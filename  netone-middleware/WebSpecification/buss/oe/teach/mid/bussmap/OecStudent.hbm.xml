<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
<class name="oe.teach.mid.buss.OecStudent" table="oec_student">
<id column="STUID" name="stuid" type="java.lang.Long">
<generator class="increment"/>
</id>
<property column="STUNAME" length="100" name="stuname" type="java.lang.String"/>
<property column="DESCRIPTION" length="1000" name="description" type="java.lang.String"/>
</class>
</hibernate-mapping>
