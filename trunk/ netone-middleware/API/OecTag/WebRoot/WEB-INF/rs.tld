	</tag>
	<tag>
		<name>accesslog</name>
		<tagclass>oe.web.tag.common.AccessLogTag</tagclass>
		<bodycontent>empty</bodycontent>
	</tag>
	<tag>
		<name>usermanage</name>
		<tagclass>oe.web.tag.common.UserManageTag</tagclass>
		<bodycontent>empty</bodycontent>
	</tag>
	<tag>
		<name>usermanageTree</name>
