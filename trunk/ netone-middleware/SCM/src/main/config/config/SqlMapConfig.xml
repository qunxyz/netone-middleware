	<sqlMap resource="com/jl/entity/xml/User.xml" />
	
	<sqlMap resource="com/jl/entity/xml/Censorship.xml" />
	<sqlMap resource="com/jl/entity/xml/File.xml" />
	<sqlMap resource="com/jl/entity/xml/Shopping.xml" />
	
	<sqlMap resource="com/jl/entity/xml/SystemConfig.xml" />
	<sqlMap resource="com/jl/entity/xml/GroupTerminal.xml" />
	<sqlMap resource="com/jl/entity/xml/Yijian.xml" />
	<sqlMap resource="com/jl/entity/xml/ContractMgr.xml"/>
	<sqlMap resource="com/jl/entity/xml/leaderView.xml"/>
	<sqlMap resource="com/jl/entity/xml/Dyform.xml" />
	
