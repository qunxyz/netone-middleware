	//根据列来计算x y坐标
	public function corXY(xoffset:int,yoffset:int,w:int,h:int):Array{
		var arr:Array=new Array();
	    var x:int=xoffset*250-w;
	    var y:int=yoffset*50-h;
	    if(x>=0){
	     arr.push(x);
	    }else{
	     x=0;
	     arr.push(x);  
	    }
	    if(y>=0){
	     arr.push(y);
	    }else{
	      y=0;
	     arr.push(y);
	    }
	   return arr;
     }
